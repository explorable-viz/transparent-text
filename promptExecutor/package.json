{
  "name": "@explorable-viz/prompt-executor",
  "version": "0.1",
  "description": "PromptExecutor is a command line tool which allows to execute the prompts into an LLM. At this time the only supported model is llama3, but further versions will offer the possibility to use also other models.\n",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/explorable-viz/transparent-text.git#automation-prompting"
  },
  "keywords": [
    "Java",
    "LLM",
    "Prompting"
  ],
  "author": "explorable-viz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/explorable-viz/fluid/issues"
  },
  "scripts": {
    "build-executor": "./script/build-executor.sh",
    "pull-llama3": "./script/model-pull.sh llama3",
    "pull-llama3-1": "./script/ollama-model-pull.sh llama3.1",
    "ollama-install": "./script/ollama-install.sh",
    "ollama-serve": "./script/ollama-serve.sh",
    "ollama-run": "./script/ollama-steps.sh",
    "generate-in-context-learning-data": "node ./script/generate-in-context-learning-data.js ./input/input-prompt-expression.json ./output/in-context-learning-data.json seed",
    "test": "./script/prompt-executor.sh it.unisa.cluelab.lllm.llm.agents.Llama31EvaluatorAgent output/in-context-learning-data.json input/settings.json testCases 0 1 1",
    "test-mock": "./script/prompt-executor.sh explorableviz.transparenttext.llm.LLMDummyAgent output/in-context-learning-data.json input/settings.json testCases 0 1 1"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "dependencies": {
    "@explorable-viz/fluid": "^0.7.61",
    "seedrandom": "^3.0.5"
  },
  "devDependencies": {
    "puppeteer": "23.2.0"
  }
}
